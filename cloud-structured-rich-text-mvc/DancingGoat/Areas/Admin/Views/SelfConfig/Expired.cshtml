@model DancingGoat.Areas.Admin.Models.SelectProjectViewModel

@{ 
    ViewBag.Title = "Expired Subscriptions";
}

<header>
    <div class="content">
        <h1>@ViewBag.Title</h1>
        <p>Your associated subscriptions in Kentico Cloud are expired. The last one expired at @ViewBag.EndAt.ToString("m").</p>
    </div>
</header>
@Html.Partial("_MessagesPartial")

@if (Model.Projects != null)
{
    <section>

        @{
            ViewBag.ButtonType = "primary";
        }

        @Html.Partial("_ProjectsPartial", Model)
    </section>
}

<div class="content sections-secondary divided">
    <section class="section-secondary">
        @{
            ViewBag.ButtonType = "secondary";
        }

        @Html.Partial("_FreePartial")
    </section>
    <section class="section-secondary">
        @{
            ViewBag.ButtonType = "secondary";
        }

        @Html.Partial("_SharedGeneralPartial")
    </section>
</div>

@section Scripts
{
    @Scripts.Render("~/Areas/Admin/Scripts/selectProject.js")
}
